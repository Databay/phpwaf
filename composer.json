{
    "name": "databay/web_application_firewall",
    "description": "Web Application Firewall",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Matheus Zych",
            "email": "mzych@databay.de"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^10"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
